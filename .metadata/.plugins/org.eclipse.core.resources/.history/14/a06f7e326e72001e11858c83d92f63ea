package numbers;
import java.util.*;
public class SumOfDigits {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n = sc.nextInt();
		int a,b=0;
		while(n!=0) {
			a=n%10;
			b+=a;
			n/=10;
		}
		System.out.println(b);

	}

}
