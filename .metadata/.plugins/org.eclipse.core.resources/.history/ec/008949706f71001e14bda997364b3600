package numbers;
import java.util.Scanner;
public class PosOrNeg {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner n= new Scanner(System.in);
		int num=n.nextInt();
		n.close();
		if(num>0)
			System.out.println("possitive");
		System.out.println("negative");
	}

}
